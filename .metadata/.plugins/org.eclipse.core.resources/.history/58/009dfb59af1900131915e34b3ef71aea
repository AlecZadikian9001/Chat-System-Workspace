package Common;
import Server.ChatServerMain;
import Client.ChatClient;


public class DualTester {
	public static void main(final String [] args){
		String arg1 = args[1];
		final String[] args2 = new String[1];
		args2[0] = arg1;
		new Thread(){
			public void run(){
				ChatServerMain.main(args2);
			}
		}.start();
		new Thread(){
			public void run(){
				ChatClient.main(args);
			}
		}.start();
		new Thread(){
			public void run(){
				ChatClient.main(args);
			}
		}.start();
		new Thread(){
			public void run(){
				ChatClient.main(args);
			}
		}.start();
	}
}
